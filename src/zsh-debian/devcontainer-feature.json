{
  "name": "zsh-debian",
  "id": "zsh-debian",
  "version": "1.0.0",
  "description": "Installs zsh with OhMyZsh on debian",
  "documentationURL": "https://github.com/pineromarcos/devcontainer-features",
  "options": {
    "theme": {
      "type": "string", 
      "default": "agnoster",
      "description": "Theme from OhMyZsh",
      "enum": [
        "agnoster",
        "robbyrussell",
        "spaceship",
        "powerlevel10k",
        "avit"
      ]
    },
    "username": {
      "type": "string", 
      "default": "root",
      "description": "Remote User to setup OhMyZsh"
    }
  }
}